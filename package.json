{
  "name": "kintone-plugin-template",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "ts-node scripts/marge-json.ts dev && kintone-plugin-packer --out dist/plugin-dev.zip plugin && node scripts/rename-ppk.ts",
    "build": "ts-node scripts/marge-json.ts prod && webpack --config webpack.prod.js",
    "dev": "ts-node scripts/marge-json.ts dev && concurrently \"yarn:dev:*\"",
    "dev:js": "webpack --config webpack.dev.js -w",
    "dev:upload": "node scripts/upload.js",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@kintone/dts-gen": "6.1.15",
    "@kintone/plugin-packer": "6.0.18",
    "@kintone/plugin-uploader": "7.1.6",
    "@kintone/rest-api-client": "3.2.0",
    "@kintone/webpack-plugin-kintone-plugin": "6.0.17",
    "@lb-ribbit/kintone-secrets": "0.2.4",
    "@lb-ribbit/kintone-xapp": "0.3.4",
    "@mui/icons-material": "5.10.9",
    "@mui/material": "5.10.12",
    "@sentry/react": "7.17.4",
    "@types/luxon": "3.1.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "concurrently": "^7.5.0",
    "dotenv": "16.0.3",
    "esbuild-loader": "2.20.0",
    "eslint": "8.27.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "immer": "9.0.16",
    "luxon": "3.1.0",
    "notistack": "2.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-feather": "^2.0.10",
    "recoil": "0.7.6",
    "terser-webpack-plugin": "5.3.6",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-merge": "5.8.0"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
