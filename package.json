{
  "name": "kintone-plugin-template",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts",
    "init": "run-s create-ppk move-ppk",
    "bundle:dev": "webpack --config webpack.dev.js",
    "bundle:prod": "webpack --config webpack.prod.js",
    "bundle:watch": "webpack --config webpack.dev.js -w",
    "merge-manifest:dev": "ts-node scripts/marge-json.ts dev",
    "merge-manifest:prod": "ts-node scripts/marge-json.ts prod",
    "develop": "run-s merge-manifest:dev bundle:dev",
    "build": "run-s  merge-manifest:prod bundle:prod",
    "watch": "run-s merge-manifest:dev bundle:watch",
    "upload": "kintone-plugin-uploader dist/plugin-dev.zip --watch --waiting-dialog-ms 3000",
    "start": "run-p watch upload",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/styled": "11.3.0",
    "@kintone/dts-gen": "5.0.6",
    "@kintone/plugin-packer": "5.0.20",
    "@kintone/plugin-uploader": "6.0.4",
    "@kintone/rest-api-client": "2.0.21",
    "@kintone/webpack-plugin-kintone-plugin": "5.0.20",
    "@mui/icons-material": "5.0.3",
    "@mui/material": "5.0.3",
    "@types/luxon": "2.0.5",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "esbuild-loader": "2.16.0",
    "eslint": "8.0.0",
    "fork-ts-checker-webpack-plugin": "6.3.4",
    "immer": "9.0.6",
    "luxon": "2.0.2",
    "notistack": "2.0.2",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.3",
    "recoil": "0.4.1",
    "terser-webpack-plugin": "5.2.4",
    "ts-node": "10.2.1",
    "typescript": "4.4.3",
    "webpack": "5.58.1",
    "webpack-cli": "4.9.0",
    "webpack-merge": "5.8.0"
  }
}
