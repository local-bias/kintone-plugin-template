{
  "name": "kintone-plugin-template",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts",
    "init": "run-s merge-manifest:prod create-ppk move-ppk",
    "bundle:dev": "webpack --config webpack.dev.js",
    "bundle:prod": "webpack --config webpack.prod.js",
    "bundle:watch": "webpack --config webpack.dev.js -w",
    "merge-manifest:dev": "ts-node scripts/marge-json.ts dev",
    "merge-manifest:prod": "ts-node scripts/marge-json.ts prod",
    "develop": "run-s merge-manifest:dev bundle:dev",
    "build": "run-s  merge-manifest:prod bundle:prod",
    "watch": "run-s merge-manifest:dev bundle:watch",
    "upload": "ts-node scripts/upload.ts",
    "start": "run-p watch upload",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/styled": "11.8.1",
    "@kintone/dts-gen": "5.0.22",
    "@kintone/plugin-packer": "5.0.38",
    "@kintone/plugin-uploader": "6.0.15",
    "@kintone/rest-api-client": "2.0.37",
    "@kintone/webpack-plugin-kintone-plugin": "5.0.38",
    "@mui/icons-material": "5.5.1",
    "@mui/material": "5.5.3",
    "@types/luxon": "2.3.1",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "dotenv": "16.0.0",
    "esbuild-loader": "2.18.0",
    "eslint": "8.12.0",
    "fork-ts-checker-webpack-plugin": "7.2.1",
    "immer": "9.0.12",
    "luxon": "2.3.1",
    "notistack": "2.0.3",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.4",
    "recoil": "0.6.1",
    "terser-webpack-plugin": "5.3.1",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  }
}
