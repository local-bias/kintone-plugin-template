{
  "name": "kintone-plugin-template",
  "version": "1.1.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip plugin && node scripts/rename-ppk.mjs",
    "build": "node scripts/marge-json.mjs prod && kbuild prod plugin --prod-root cdn && kintone-plugin-packer --out dist/plugin.zip --ppk dist/private.ppk plugin",
    "standalone": "node scripts/marge-json.mjs standalone && kbuild prod plugin --prod-root plugin && kintone-plugin-packer --out dist/plugin-standalone.zip --ppk dist/private.ppk plugin",
    "dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip --ppk dist/private.ppk plugin && concurrently \"npm:dev:*\"",
    "dev:build": "kbuild dev plugin",
    "dev:upload": "node scripts/upload.mjs",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@kintone/dts-gen": "7.0.4",
    "@kintone/plugin-packer": "7.0.4",
    "@kintone/plugin-uploader": "8.0.0",
    "@kintone/rest-api-client": "4.1.0",
    "@konomi-app/kbuild": "^2.0.0",
    "@konomi-app/kintone-utilities": "^2.13.0",
    "@konomi-app/kintone-utility-component": "^1.2.0",
    "@konomi-app/ui": "^1.7.0",
    "@konomi-app/ui-react": "^1.1.1",
    "@lb-ribbit/kintone-secrets": "0.3.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.14.6",
    "@mui/material": "5.14.6",
    "@types/luxon": "3.3.1",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "concurrently": "^8.2.0",
    "dotenv": "16.3.1",
    "eslint": "8.47.0",
    "immer": "10.0.2",
    "luxon": "3.4.2",
    "notistack": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-feather": "^2.0.10",
    "recoil": "0.7.7",
    "typescript": "5.1.6"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
