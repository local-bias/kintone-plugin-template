{
  "name": "kintone-plugin-template",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "develop": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.dev.js -w",
    "init": "run-s create-ppk move-ppk",
    "start": "run-p watch upload",
    "upload": "kintone-plugin-uploader dist/plugin-dev.zip --watch --waiting-dialog-ms 3000",
    "update-modules": "ncu -u",
    "check-updates": "ncu",
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/styled": "11.3.0",
    "@kintone/dts-gen": "5.0.5",
    "@kintone/plugin-packer": "5.0.19",
    "@kintone/plugin-uploader": "6.0.4",
    "@kintone/rest-api-client": "2.0.20",
    "@kintone/webpack-plugin-kintone-plugin": "5.0.19",
    "@mui/icons-material": "5.0.1",
    "@mui/material": "5.0.2",
    "@types/luxon": "2.0.4",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "esbuild-loader": "2.15.1",
    "eslint": "7.32.0",
    "fork-ts-checker-webpack-plugin": "6.3.3",
    "immer": "9.0.6",
    "luxon": "2.0.2",
    "notistack": "2.0.2",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.3",
    "recoil": "0.4.1",
    "terser-webpack-plugin": "5.2.4",
    "ts-node": "10.2.1",
    "typescript": "4.4.3",
    "webpack": "5.56.1",
    "webpack-cli": "4.8.0",
    "webpack-merge": "5.8.0"
  }
}
